# perform IEEE 1180 test series
# Typical usage:
#    doieee  >outfile
# where progname is ieeetest or a variant

for i in 1 2 3 4 5; 
do
time ./ieeetest $i -256 255 1 10000
time ./ieeetest $i -5 5     1 10000
time ./ieeetest $i -300 300 1 10000

time ./ieeetest $i -256 255 -1 10000
time ./ieeetest $i -5 5     -1 10000
time ./ieeetest $i -300 300 -1 10000
done
